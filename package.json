{
    "name": "my-bot",
    "version": "1.0.0",
    "author": "Kevin Novak",
    "description": "A discord.js bot template written with TypeScript",
    "license": "MIT",
    "private": true,
    "engines": {
        "node": ">=16.6.0"
    },
    "type": "module",
    "exports": [
        "./dist/start-bot.js",
        "./dist/start-manager.js"
    ],
    "scripts": {
        "lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint . --fix --cache --ext .js,.jsx,.ts,.tsx",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "prebuild": "shx rm -rf dist",
        "build": "tsc --project tsconfig.json",
        "prepublishOnly": "npm run lint && npm run format:fix && npm run build",
        "commands:register": "npm run build && node --enable-source-maps dist/start-bot.js --register",
        "commands:register:prod": "cross-env NODE_ENV=production npm run commands:register",
        "commands:clear": "npm run build && node --enable-source-maps dist/start-bot.js --clear",
        "commands:clear:prod": "cross-env NODE_ENV=production npm run commands:clear",
        "dev": "npm run build && cross-env NODE_ENV=development npm run start:bot",
        "dev:manager": "npm run build && cross-env NODE_ENV=development npm run start:manager",
        "prod": "npm run build && cross-env NODE_ENV=production npm run start:manager",
        "start": "npm run dev:manager",
        "start:bot": "node --enable-source-maps dist/start-bot.js",
        "start:manager": "node --enable-source-maps dist/start-manager.js",
        "start:pm2": "npm run pm2:start",
        "pm2:start": "pm2 start process.json",
        "pm2:stop": "pm2 stop process.json",
        "pm2:delete": "pm2 delete process.json"
    },
    "dependencies": {
        "@discordjs/rest": "^1.5.0",
        "cheerio": "^1.0.0-rc.12",
        "class-transformer": "0.5.1",
        "class-validator": "^0.14.0",
        "config": "^3.3.8",
        "cron": "^2.1.0",
        "discord.js": "^14.7.1",
        "discord.js-rate-limiter": "1.3.2",
        "emoji-regex": "^10.2.1",
        "express": "^4.18.2",
        "express-promise-router": "4.1.1",
        "filesize": "^9.0.11",
        "firebase-admin": "^11.4.1",
        "fireorm": "^0.23.3",
        "json-templates": "^4.2.0",
        "linguini": "1.3.1",
        "lodash-es": "^4.17.21",
        "luxon": "^3.2.1",
        "mingo": "^6.2.0",
        "moment": "^2.29.4",
        "node-cache": "^5.1.2",
        "node-fetch": "^3.3.0",
        "pino": "^8.8.0",
        "pino-abstract-transport": "^1.0.0",
        "pino-pretty": "^9.1.1",
        "pm2": "^5.2.2",
        "random-weighted-choice": "^0.1.4",
        "re2": "^1.18.0",
        "reflect-metadata": "^0.1.13",
        "rss-parser": "^3.12.0",
        "rxjs": "^7.8.0",
        "type-fest": "^3.4.0",
        "url-regex-safe": "^3.0.0",
        "v8-profiler-next": "^1.9.0"
    },
    "devDependencies": {
        "@types/config": "^3.3.0",
        "@types/cron": "^2.0.0",
        "@types/express": "^4.17.15",
        "@types/lodash-es": "^4.17.6",
        "@types/luxon": "^3.1.0",
        "@types/node": "^16.11.45",
        "@types/node-fetch": "3.0.2",
        "@types/remove-markdown": "0.3.1",
        "@types/url-regex-safe": "^1.0.0",
        "@typescript-eslint/eslint-plugin": "^5.47.0",
        "@typescript-eslint/parser": "^5.47.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.30.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-unicorn": "^45.0.2",
        "prettier": "^2.8.1",
        "shx": "^0.3.4",
        "typescript": "^4.9.4"
    }
}
